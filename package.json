        "stylelint-use-nesting": "^5.0.0",
        "stylelint-value-no-exposed-custom-properties": "^2.3.1",
        "typescript": "^5.2.2",
        "vitest": "^1.6.1"
    },
    "dependencies": {
        "next": "^14.2.26",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
